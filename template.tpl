___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const injectScript = require('injectScript');
const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');

const sdkUrl = 'https://cdn.uniqode.com/insights/production/v1/latest/sdk.js';
const clientKey = data.clientKey;

const METHODS = [
  'track','identify','getUserId','set','setOnce','unset',
  'add','append','prepend','preInsert','postInsert','remove',
  'addPlugin','removePlugin','processQueuedEvents'
];

let insights = copyFromWindow('insights');
let queue = [];

if (!insights) {
  insights = {};

  METHODS.forEach(function(method) {
    insights[method] = function() {
      queue.push({ method: method, args: arguments });
    };
  });

  insights.isReady = false;

  insights.init = function(config) {
    insights._config = config;
  };

  setInWindow('insights', insights, true);
}

injectScript(sdkUrl,
  

  function onSuccess() {
    callInWindow('insights.init', [{ clientKey: clientKey }]);

    insights.isReady = true;

    queue.forEach(function(item) {
      callInWindow('insights.' + item.method, item.args);
    });

    queue = [];

    data.gtmOnSuccess();
  },
  function onFailure() {
    data.gtmOnFailure();
  }
);


